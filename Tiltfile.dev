custom_build(
  ref = '169942020521.dkr.ecr.eu-west-1.amazonaws.com/local/insolvency-api',
  command = 'DOCKER_BUILDKIT=0 docker build --build-arg SSH_PRIVATE_KEY="$(cat ~/.ssh/id_rsa)" --build-arg SSH_PRIVATE_KEY_PASSPHRASE --tag $EXPECTED_REF .',
  deps = [
    './',
  ],
  ignore = [
    'LICENSE',
    'Makefile',
    'Readme',
    '.gitignore',
    '.dockerignore',
    'version'
  ]
)